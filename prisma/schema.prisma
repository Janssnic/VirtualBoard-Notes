generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Notes {
  id       Int     @id @default(autoincrement())
  title    String? @db.VarChar
  content  String? @db.VarChar
  x_loc    Int?
  y_loc    Int?
  board_id Int?
}
